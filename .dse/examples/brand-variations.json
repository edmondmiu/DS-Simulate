{
  "$comment": "Example DSE configuration with extensive brand-specific customizations",
  "colorLibrary": {
    "colorSpace": "oklch",
    "lightnessRange": {
      "min": 15,
      "max": 95
    },
    "chromaRange": {
      "primary": 0.15,
      "neutral": 0.05
    },
    "accessibilityThresholds": {
      "AA": 4.5,
      "AAA": 7.0
    },
    "conversionOptions": {
      "outputFormat": "hex",
      "preserveOriginal": true
    },
    "brandSpecific": {
      "bet9ja": {
        "lightnessAdjustment": 5,
        "chromaMultiplier": 1.3,
        "hueShift": 15
      },
      "sporty": {
        "lightnessAdjustment": -3,
        "chromaMultiplier": 1.5,
        "hueShift": 45
      },
      "premium": {
        "lightnessAdjustment": 8,
        "chromaMultiplier": 0.8,
        "hueShift": 0
      },
      "youth": {
        "lightnessAdjustment": -5,
        "chromaMultiplier": 1.8,
        "hueShift": 180
      }
    },
    "colorGeneration": {
      "rampSteps": 9,
      "preserveKeyColors": true,
      "algorithmicGeneration": true
    }
  }
}