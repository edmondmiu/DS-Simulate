{
  "$comment": "Minimal DSE configuration for basic OKLCH support",
  "colorLibrary": {
    "colorSpace": "oklch",
    "lightnessRange": {
      "min": 15,
      "max": 95
    },
    "chromaRange": {
      "primary": 0.15,
      "neutral": 0.05
    },
    "accessibilityThresholds": {
      "AA": 4.5,
      "AAA": 7.0
    },
    "conversionOptions": {
      "outputFormat": "hex",
      "preserveOriginal": true
    }
  }
}