{
  "$comment": "Example DSE configuration focused on maximum accessibility compliance",
  "colorLibrary": {
    "colorSpace": "oklch",
    "lightnessRange": {
      "min": 20,
      "max": 90
    },
    "chromaRange": {
      "primary": 0.12,
      "neutral": 0.03
    },
    "accessibilityThresholds": {
      "AA": 4.5,
      "AAA": 7.0
    },
    "conversionOptions": {
      "outputFormat": "hex",
      "preserveOriginal": true
    },
    "colorGeneration": {
      "rampSteps": 7,
      "preserveKeyColors": true,
      "algorithmicGeneration": false
    }
  }
}